{
  "name": "siyuan-database-properties-panel",
  "version": "0.3.0",
  "type": "module",
  "description": "Plugin for Siyuan (https://b3log.org/siyuan). Provides a panel to view database properties of a page created from a database.",
  "repository": "https://github.com/Macavity/siyuan-database-properties-panel",
  "homepage": "https://github.com/Macavity/siyuan-database-properties-panel",
  "author": "Macavity",
  "license": "MIT",
  "scripts": {
    "make-link": "node --no-warnings ./scripts/make_dev_link.js",
    "make-link-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/elevate.ps1 -scriptPath ./scripts/make_dev_link.js",
    "dev": "NODE_ENV=development vite build --watch",
    "update-version": "node --no-warnings ./scripts/update_version.js",
    "build": "vite build",
    "make-install": "vite build && node --no-warnings ./scripts/make_install.js"
  },
  "dependencies": {
    "@eslint/js": "^9.12.0",
    "@sentry/browser": "^8.34.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^4.0.1",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "dayjs": "^1.11.13",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-svelte": "^2.44.1",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.0",
    "globals": "^15.11.0",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "prettier": "^3.3.3",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.63.3",
    "siyuan": "1.0.3",
    "svelte": "^4.2.0",
    "svelte-eslint-parser": "^0.41.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^1.0.2",
    "vite-plugin-zip-pack": "^1.0.5"
  }
}